{
  "name": "typos",
  "version": "1.0.1",
  "description": "calculate typos count on text writing",
  "main": "lib/typos.js",
  "scripts": {
    "wisdom": "npm run build",
    "build": "redrun build:*",
    "lint:eslint": "eslint lib",
    "lint": "redrun lint:*",
    "test": "tape 'test/**/*.js'",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:lint": "npm run watcher -- npm run lint",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "build:client": "webpack --progress",
    "watch:client": "npm run build:client -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/typos"
  },
  "keywords": [
    "extract",
    "bzip2",
    "bz2",
    "pack",
    "tar"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/typos/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "homepage": "https://github.com/coderaiser/typos",
  "dependencies": {
    "emitify": "^3.0.1",
    "inherits": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "redrun": "^5.9.2",
    "tape": "^4.6.2",
    "webpack": "^3.10.0"
  }
}
